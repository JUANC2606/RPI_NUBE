    {
        "id": "9743a68c22620124",
        "type": "function",
        "z": "d3bafc4d888d1327",
        "g": "db0763cc983c9bdb",
        "name": "Controlador PI Local",
        "func": "// % Controlador PI\n// u(k) = b0_c * e(k) + b1_c * e(k - 1) + u(k - 1);\n\n// variables del PI\n// % Parámetros del controlador PI\nvar Kp = 2.54e-6;\nvar Ki = 1.68e-6;\nvar T = global.get('TiempoMuestreo')||1; //% Tiempo de muestreo\nvar b0PI = Kp + Ki * T;\nvar b1PI = -Kp;\n// variables del calculo del error\nvar error = msg.payload;\nvar error_k1 = context.get('error_k1','memoryOnly')||0;\nvar u1 = context.get('u1','memoryOnly')||0;\n\n\n// ecuacion de control  PI\nvar u = b0PI * error + b1PI * error_k1 + u1;\n\nmsg.payload = u;\nnode.status({ fill: \"blue\", shape: \"ring\", text: \"u\" + u });\n\n// consulta de variables de monitoreo\ncontext.set('monitoreo', { 'errores': { error, error_k1 }, 'ganancia': { b0PI, b1PI},'salidasControl':{u,u1}}, 'memoryOnly');\n// guardar el error anterior\ncontext.set('error_k1', error , 'memoryOnly');\n// guardar la salida anterior del PI\ncontext.set('u1', u , 'memoryOnly');\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 600,
